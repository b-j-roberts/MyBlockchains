{
  "name": "naive-crypto-l2-contracts",
  "version": "1.0.0",
  "description": "Naive Crypto L2 Contracts",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "solc --bin --abi -o builds contracts/*.sol",
    "deploy-private-local": "./scripts/deploy-private-local.sh && node scripts/deploy_private.js",
    "deploy-goerli": "node scripts/deploy_goerli.js",
    "test-private": "mocha tests/transaction-storage.private.test.js --timeout 30000",
    "test-goerli": "mocha tests/transaction-storage.goerli.test.js --timeout 30000",
    "go-gen": "rm -rf go/ && mkdir -p go/ && abigen --bin=builds/TransactionStorage.bin --abi=builds/TransactionStorage.abi --pkg=contracts --out=go/transaction-storage.go"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "shelljs": "^0.8.5",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.9.0"
  }
}
