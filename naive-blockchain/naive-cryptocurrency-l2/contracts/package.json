{
  "name": "naive-crypto-l2-contracts",
  "version": "1.0.0",
  "description": "Naive Crypto L2 Contracts",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "solcjs --base-path . --include-path node_modules/ --bin --abi --optimize -o builds/ contracts/*.sol",
    "build-bridge": "solcjs --base-path . --include-path node_modules/ --bin --abi --optimize -o builds/ contracts/bridge/*.sol",
    "build-tokens": "solcjs --base-path . --include-path node_modules/ --bin --abi --optimize -o builds/ contracts/tokens/*/*.sol",
    "deploy-private": "node scripts/deploy_private.js",
    "deploy-goerli": "node scripts/deploy_goerli.js",
    "deploy-l2": "node scripts/deploy_l2.js",
    "deploy-l2-tokens": "node scripts/deploy_l2_tokens.js",
    "test-private": "mocha tests/transaction-storage.private.test.js --timeout 30000",
    "test-goerli": "mocha tests/transaction-storage.goerli.test.js --timeout 30000",
    "go-gen": "./scripts/go-gen.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2",
    "shelljs": "^0.8.5",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.9.0"
  }
}
